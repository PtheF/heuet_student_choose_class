view.route-app{
    width: 100vw;
    height: 100vh;
    background-color: rgb(65, 65, 95);
}

swiper-item.swp-item{
    position: relative;
}

swiper-item.swp-item text{
    position: absolute;
    bottom: 25px;
    padding-left: 10px;
    color: white;
}

image.swiper-img{
    width: 100%;
    height: 160px;
    display: block;
}

view.route-title{
    text-align: center;
    font-size: 25px;
    color: white;
}

view.route-wel{
    text-align: center;
    color: white;
    margin: 5px auto 0;
}

view.func{
    margin: 40px auto 0;
}

view.button-view{
    margin: 10px auto 0;
}

